public class main {
    public static void main(String[] args) {

        // 1 way of creating the array.
        int arr1[] = {10,20,30,40,50};

        // 2 way of creating the array.
        int arr2[] = new int [5];
        arr2[0]=1;
        arr2[1]=20;
        arr2[2]=30;
        arr2[3]=40;
        arr2[4]=50;
        
    }
}

